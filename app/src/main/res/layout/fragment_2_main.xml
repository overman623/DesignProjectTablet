<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >



    <include
        android:id="@+id/include_title"
        layout="@layout/include_title_mode_select"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height">
    </include>

    <FrameLayout
        android:id="@+id/fragment_main_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/radio_group_bottom"
        android:layout_below="@+id/include_title"
        >
    </FrameLayout>


    <RelativeLayout
        android:id="@+id/layout_inflated"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_above="@+id/radio_group_bottom"
        />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/darkgray"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="120dp"
        ></FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:background="@color/black"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >

            <katech.frame.ButtonLayout
                android:id="@+id/btn_goto_home"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                >
            </katech.frame.ButtonLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color_3"
                android:text="홈으로"
                android:gravity="center"
                android:layout_alignParentBottom="true"
                android:textSize="20dp"
                android:layout_marginBottom="10dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >

            <ImageView
                android:id="@+id/img_find_load_sub"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp" />

            <ImageView
                android:id="@+id/img_find_load"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color_3"
                android:text="길찾기"
                android:gravity="center"
                android:layout_alignParentBottom="true"
                android:textSize="20dp"
                android:layout_marginBottom="10dp"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >

            <ImageView
                android:id="@+id/img_freeze_sub"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp" />

            <ImageView
                android:id="@+id/img_freeze"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color_3"
                android:text="냉난방"
                android:gravity="center"
                android:layout_alignParentBottom="true"
                android:textSize="20dp"
                android:layout_marginBottom="10dp"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >

            <ImageView
                android:id="@+id/img_guide_changeable_sub"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp" />

            <ImageView
                android:id="@+id/img_guide_changeable"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                />

            <TextView
                android:id="@+id/text_guide_changeable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color_3"
                android:text=""
                android:gravity="center"
                android:layout_alignParentBottom="true"
                android:textSize="20dp"
                android:layout_marginBottom="10dp"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >

            <ImageView
                android:id="@+id/img_music_sub"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp" />

            <ImageView
                android:id="@+id/img_music"
                android:layout_width="@dimen/bottom_icon_size"
                android:layout_height="@dimen/bottom_icon_size"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color_3"
                android:text="음악"
                android:gravity="center"
                android:layout_alignParentBottom="true"
                android:textSize="20dp"
                android:layout_marginBottom="10dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingTop="7dp"
            android:paddingBottom="7dp"
            >

            <katech.frame.ToggleButtonLayout
                android:id="@+id/toggle_button_layout"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                >
            </katech.frame.ToggleButtonLayout>

            <FrameLayout
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/darkgray"
                android:layout_centerVertical="true"
                ></FrameLayout>

        </RelativeLayout>
    </LinearLayout>

    <include
        layout="@layout/include_fragment_main_bottom_radio"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        >
    </include>


</RelativeLayout>
